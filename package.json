{
  "name": "@ff00ff/mammoth-cli",
  "version": "0.1.0",
  "author": "Martijn de Haan @martijndeh",
  "bin": {
    "mammoth": "./bin/run"
  },
  "bugs": "https://github.com/ff00ff/mammoth-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@pgql/parse": "^1.2.6",
    "pg": "^8.4.1",
    "pgsql-parser": "^0.8.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.39",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ff00ff/mammoth-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mammoth",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ff00ff/mammoth-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "go": "ts-node ./src/test.ts"
  },
  "types": "lib/index.d.ts"
}
